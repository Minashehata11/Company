@model ApplicationUser
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Edit</h1>
<div class="row">
    <div class="col-6">
        @Html.ValidationSummary(true,"",new {@class="text-danger"})
        <form asp-action="Edit" asp-controller="Users" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="@Model.UserName" class="col-form-label"> Code</label>
                <input asp-for="@Model.UserName" class="form-control" />
                <span asp-validation-for="@Model.UserName" class="text-danger"></span>

            </div>
           
            <br />
            <input type="submit" value="Update" class="btn btn-primary" />
            <a asp-action="Index" class="btn btn-light">Back To List</a>

        </form>

    </div>
    @section Scripts{
        <partial name="_ValidationScriptsPartial" />
    }

</div>
