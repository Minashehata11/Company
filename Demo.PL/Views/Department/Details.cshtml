@model Department

<div class="row">
<div class="col-6">
   
        <div class="form-group">
            <label asp-for="@Model.Code" class="col-form-label"> Code</label>
        <input asp-for="@Model.Code" class="form-control" readonly />
           

        </div>
        <div class="form-group">
            <label asp-for="@Model.Name" class="col-form-label"> Name</label>
        <input asp-for="@Model.Name" class="form-control" readonly />
          

        </div>
        <br />
       
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-light">Update</a>
        <a asp-action="Index"> Back</a>

</div>