@model Department
<div class="row">
    <div class="col-6">
        <form asp-action="Delete" asp-controller="Department">
            <div class="form-group">
                <label asp-for="@Model.Code" class="col-form-label"> Code</label>
                <input asp-for="@Model.Code" class="form-control" readonly />
                

            </div>
            <div class="form-group">
                <label asp-for="@Model.Name" class="col-form-label"> Name</label>
                <input asp-for="@Model.Name" class="form-control" readonly />
                

            </div>
            <br />
            <input type="submit" value="Delete" class="btn btn-danger" />
            <a asp-action="Index" class="btn btn-light">Back To List</a>

        </form>

    </div>
   
</div>